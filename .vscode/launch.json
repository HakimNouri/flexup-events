{
    "configurations": [
        {
            "name": "Python Debugger (Fab): Runserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
        },
        {
            "name": "Django Test: Alphonse Journey",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "test",
                "_documentation.user_journeys.alphonse"
            ],
            "django": true,
            "justMyCode": true,
            "env": {
                "DEBUG": "True"
            }
        },
